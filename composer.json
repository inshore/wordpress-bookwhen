{
    "name": "inshore/wordpress-bookwhen",
    "description": "WordPress plugin built on the inshore/bookwhen PHP API SDK package",
    "type": "wordpress-plugin",
    "require": {
        "inshore/bookwhen": "dev-develop",
        "humbug/php-scoper": "dev-main"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Inshore\\WordpressBookwhen\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Daniel Mullin",
            "email": "email@danielmullin.com"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts" : {
	"zip" : "zip -r inshore-bookwhen.zip inshore-bookwhen"
    }
}
